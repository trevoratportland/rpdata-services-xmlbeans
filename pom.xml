<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">    <modelVersion>4.0.0</modelVersion>    <groupId>com.rpdata</groupId>    <artifactId>rpdata-services-xmlbeans</artifactId>    <packaging>jar</packaging>    <version>1.0.0-SNAPSHOT</version>    <name>rpdata-services-xmlbeans</name>    <!--  -->    <!--  SVN Repositories -->    <!--  -->    <scm>        <connection>scm:git:git@github.com:portlandrisk/rpdata-services-xmlbeans.git</connection>        <url>scm:git:git@github.com:portlandrisk/rpdata-services-xmlbeans.git</url>    </scm>    <!--  -->    <!--  Properties -->    <!--  -->    <properties>        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>        <src.java.root>${basedir}/src/main/java</src.java.root>        <src.generated.root>${src.java.root}/com/rpdata/model/generated</src.generated.root>        <bindings.src.dir>src/main/resources/bindings</bindings.src.dir>        <bindings.rpdata.file>jaxb-rpdata.xjb</bindings.rpdata.file>        <rpdata.root.url>http://rpp.rpdata.com/bsgAU-2.0/ws</rpdata.root.url>        <rpdata.au.government.stats.url>${rpdata.root.url}/auGovernmentStatisticsService.wsdl</rpdata.au.government.stats.url>        <rpdata.market.compare.url>${rpdata.root.url}/marketCompareService.wsdl</rpdata.market.compare.url>        <rpdata.on.market.url>${rpdata.root.url}/onTheMarketService.wsdl</rpdata.on.market.url>        <rpdata.property.details.url>${rpdata.root.url}/propertyService.wsdl</rpdata.property.details.url>        <rpdata.property.imagery.url>${rpdata.root.url}/propertyImageryService.wsdl</rpdata.property.imagery.url>        <rpdata.property.stats.url>${rpdata.root.url}/propertyStatisticsService.wsdl</rpdata.property.stats.url>        <rpdata.property.search.url>${rpdata.root.url}/propertySearchService.wsdl</rpdata.property.search.url>        <rpdata.sales.url>${rpdata.root.url}/salesService.wsdl</rpdata.sales.url>        <rpdata.session.url>${rpdata.root.url}/sessionService.wsdl</rpdata.session.url>        <rpdata.user.url>${rpdata.root.url}/userService.wsdl</rpdata.user.url>        <rpdata.valuers.url>${rpdata.root.url}/valuersService.wsdl</rpdata.valuers.url>        <!-- -->        <!-- Since there are no namespaces on the objects place them in the single package. Not good but...-->        <!-- -->        <rpdata.root.package.name>com.rpdata.model.generated</rpdata.root.package.name>    </properties>    <!--  -->    <!--  Repositories -->    <!--  -->    <distributionManagement>        <repository>            <id>deployment</id>            <name>Internal Releases</name>            <url>https://devapps.encompasshost.com/nexus/content/repositories/releases/</url>        </repository>        <snapshotRepository>            <id>deployment</id>            <name>Internal Snapshots</name>            <url>https://devapps.encompasshost.com/nexus/content/repositories/snapshots/</url>        </snapshotRepository>    </distributionManagement>    <!--  -->    <!--  Dependencies -->    <!--  -->    <dependencies>    </dependencies>    <!--  -->    <!--  Build -->    <!--  -->    <build>        <plugins>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-dependency-plugin</artifactId>                <version>2.2</version>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-javadoc-plugin</artifactId>                <version>2.8.1</version>                <configuration>                    <additionalparam>-source 1.6</additionalparam>                </configuration>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-clean-plugin</artifactId>                <version>2.4.1</version>                <configuration>                    <filesets>                        <fileset>                            <directory>${src.generated.root}</directory>                            <includes>                                <include>**/*</include>                            </includes>                        </fileset>                    </filesets>                </configuration>            </plugin>            <plugin>                <groupId>org.jvnet.jax-ws-commons</groupId>                <artifactId>jaxws-maven-plugin</artifactId>                <version>2.2.1</version>                <executions>                    <execution>                        <id>wsimport-rpdata-all</id>                        <goals>                            <goal>wsimport</goal>                        </goals>                        <configuration>                            <staleFile>${project.build.directory}/jaxb2/.xjcRPDataStaleFlag</staleFile>                            <extension>true</extension>                            <bindingDirectory>${bindings.src.dir}</bindingDirectory>                            <bindingFiles>                                <bindingFile>${bindings.rpdata.file}</bindingFile>                            </bindingFiles>                            <sourceDestDir>${src.java.root}</sourceDestDir>                            <packageName>${rpdata.root.package.name}</packageName>                            <wsdlUrls>                                <wsdlUrl>${rpdata.au.government.stats.url}</wsdlUrl>                                <wsdlUrl>${rpdata.market.compare.url}</wsdlUrl>                                <wsdlUrl>${rpdata.on.market.url}</wsdlUrl>                                <wsdlUrl>${rpdata.property.details.url}</wsdlUrl>                                <wsdlUrl>${rpdata.property.imagery.url}</wsdlUrl>                                <wsdlUrl>${rpdata.property.stats.url}</wsdlUrl>                                <wsdlUrl>${rpdata.property.search.url}</wsdlUrl>                                <wsdlUrl>${rpdata.sales.url}</wsdlUrl>                                <wsdlUrl>${rpdata.session.url}</wsdlUrl>                                <wsdlUrl>${rpdata.user.url}</wsdlUrl>                                <wsdlUrl>${rpdata.valuers.url}</wsdlUrl>                            </wsdlUrls>                            <xjcArgs>                                <xjcArg>-Xcollection-setter-injector</xjcArg>                            </xjcArgs>                        </configuration>                    </execution>                </executions>                <dependencies>                    <dependency>                        <groupId>net.java.dev.vcc.thirdparty</groupId>                        <artifactId>collection-setter-injector</artifactId>                        <version>0.5.0-1</version>                    </dependency>                    <dependency>                        <groupId>com.sun.xml.bind</groupId>                        <artifactId>jaxb-xjc</artifactId>                        <version>2.2.5</version>                    </dependency>                    <dependency>                        <groupId>com.sun.xml.bind</groupId>                        <artifactId>jaxb-impl</artifactId>                        <version>2.2.5</version>                    </dependency>                </dependencies>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-deploy-plugin</artifactId>                <version>2.5</version>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-source-plugin</artifactId>                <version>2.1.2</version>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-compiler-plugin</artifactId>                <version>2.3.2</version>                <inherited>true</inherited>                <configuration>                    <source>1.6</source>                    <target>1.6</target>                </configuration>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-release-plugin</artifactId>                <version>2.4.1</version>                <configuration>                    <goals>deploy</goals>                </configuration>            </plugin>        </plugins>    </build></project>